<constraint-mappings
	xmlns="http://jboss.org/xml/ns/javax/validation/mapping"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.org/xml/ns/javax/validation/mapping validation-mapping-1.0.xsd">
	<default-package>
		jpa.entity
	</default-package>
	<bean class="BeanValXMLEntity" ignore-annotations="false">
		<field name="name">
			<!--
				@NotNull(message="XMLBasicEntity.name is null")
			-->
			<constraint
				annotation="javax.validation.constraints.NotNull">
				<message>BeanValXMLEntity.name is null</message>
			</constraint>
		</field>
	</bean>

</constraint-mappings>
